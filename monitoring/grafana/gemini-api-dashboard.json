{
  "dashboard": {
    "title": "Gemini API - Production Monitoring",
    "description": "Real-time monitoring dashboard for Google Gemini API integration",
    "tags": ["gemini", "ai", "api", "production"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate (req/sec)",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(gemini_api_requests_total[1m])",
            "legendFormat": "Request Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Requests/sec",
            "format": "reqps"
          }
        ],
        "alert": {
          "name": "High API Request Rate",
          "conditions": [
            {
              "evaluator": {
                "params": [10],
                "type": "gt"
              },
              "query": {
                "params": ["A", "5m", "now"]
              }
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "message": "Gemini API request rate exceeded 10 req/sec threshold"
        }
      },
      {
        "id": 2,
        "title": "Error Rate (%)",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(gemini_api_errors_total[5m]) / rate(gemini_api_requests_total[5m]) * 100",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Error %",
            "format": "percent",
            "max": 100
          }
        ],
        "alert": {
          "name": "High Error Rate",
          "conditions": [
            {
              "evaluator": {
                "params": [5],
                "type": "gt"
              },
              "query": {
                "params": ["A", "5m", "now"]
              }
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "message": "Gemini API error rate exceeded 5% threshold"
        }
      },
      {
        "id": 3,
        "title": "Response Time (p50, p95, p99)",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(gemini_api_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(gemini_api_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(gemini_api_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "label": "Duration",
            "format": "s"
          }
        ],
        "alert": {
          "name": "Slow API Response Time",
          "conditions": [
            {
              "evaluator": {
                "params": [3],
                "type": "gt"
              },
              "query": {
                "params": ["B", "5m", "now"]
              }
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "message": "Gemini API p95 response time exceeded 3s SLA"
        }
      },
      {
        "id": 4,
        "title": "Circuit Breaker Status",
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "gemini_circuit_breaker_state",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "textMode": "value",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "type": "value",
                "options": {
                  "0": {
                    "text": "CLOSED",
                    "color": "green"
                  },
                  "1": {
                    "text": "OPEN",
                    "color": "red"
                  },
                  "2": {
                    "text": "HALF_OPEN",
                    "color": "orange"
                  }
                }
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 1,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Cache Hit Rate (%)",
        "type": "stat",
        "gridPos": {
          "x": 18,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "rate(gemini_cache_hits_total[5m]) / (rate(gemini_cache_hits_total[5m]) + rate(gemini_cache_misses_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 50,
                  "color": "orange"
                },
                {
                  "value": 80,
                  "color": "green"
                }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Fallback Usage (OpenAI)",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(gemini_fallback_usage_total[5m])",
            "legendFormat": "Fallback Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Fallbacks/sec",
            "format": "reqps"
          }
        ],
        "alert": {
          "name": "High Fallback Usage",
          "conditions": [
            {
              "evaluator": {
                "params": [1],
                "type": "gt"
              },
              "query": {
                "params": ["A", "5m", "now"]
              }
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "message": "Gemini API experiencing high fallback rate to OpenAI"
        }
      },
      {
        "id": 7,
        "title": "Token Consumption (TPM)",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(gemini_tokens_consumed_total[1m]) * 60",
            "legendFormat": "Tokens Per Minute",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "TPM",
            "format": "short"
          }
        ]
      },
      {
        "id": 8,
        "title": "Rate Limit Status",
        "type": "table",
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "gemini_rate_limit_remaining",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {
                "window": 0,
                "remaining": 1,
                "limit": 2
              }
            }
          }
        ]
      },
      {
        "id": 9,
        "title": "Error Types Distribution",
        "type": "piechart",
        "gridPos": {
          "x": 0,
          "y": 24,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (error_type) (increase(gemini_api_errors_total[1h]))",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 10,
        "title": "Daily Request Volume",
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 24,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(increase(gemini_api_requests_total[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 11,
        "title": "API Uptime (24h)",
        "type": "stat",
        "gridPos": {
          "x": 18,
          "y": 24,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "(1 - (sum(increase(gemini_api_errors_total[24h])) / sum(increase(gemini_api_requests_total[24h])))) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 95,
                  "color": "orange"
                },
                {
                  "value": 99,
                  "color": "green"
                }
              ]
            }
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "custom",
          "options": [
            {
              "text": "Production",
              "value": "production",
              "selected": true
            },
            {
              "text": "Staging",
              "value": "staging"
            },
            {
              "text": "Development",
              "value": "development"
            }
          ]
        },
        {
          "name": "interval",
          "type": "interval",
          "options": [
            "1m",
            "5m",
            "10m",
            "30m",
            "1h",
            "6h",
            "12h",
            "1d"
          ],
          "auto": true,
          "auto_count": 30,
          "auto_min": "10s"
        }
      ]
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "-- Grafana --",
          "enable": true,
          "iconColor": "blue",
          "tags": ["deployment"]
        },
        {
          "name": "Incidents",
          "datasource": "-- Grafana --",
          "enable": true,
          "iconColor": "red",
          "tags": ["incident"]
        }
      ]
    }
  }
}
